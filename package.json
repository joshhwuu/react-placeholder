{
  "name": "react-placeholder",
  "version": "4.1.0",
  "description": "A React component to easily replicate your page with nice placeholders while the content is loading",
  "main": "lib",
  "scripts": {
    "test": "jest",
    "build": "rimraf lib && mkdir lib && sass src:lib && tsc -p tsconfig.build.json",
    "preversion": "npm run typecheck && npm run test",
    "prepublishOnly": "npm run build",
    "start": "styleguidist server",
    "typecheck": "tsc --noEmit",
    "release-version": "smooth-release"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:buildo/react-placeholder"
  },
  "keywords": [
    "react",
    "react-component",
    "placeholder",
    "filler",
    "loading",
    "paragraph"
  ],
  "author": "Francesco Cioria <francesco@buildo.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/buildo/react-placeholder/issues"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/buildo/react-placeholder",
  "typings": "lib",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@cfaester/enzyme-adapter-react-18": "^0.7.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "babel-loader": "^9.1.2",
    "cheerio": "^1.0.0-rc.12",
    "css-loader": "^6.7.3",
    "enzyme": "^3.11.0",
    "file-loader": "^6.2.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^30.0.5",
    "lodash": "^4.17.21",
    "progress-bar-webpack-plugin": "^2.1.0",
    "raf": "^3.4.1",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-docgen-typescript": "^1.16.1",
    "react-dom": "^18.2.0",
    "react-styleguidist": "^13.1.4",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.0",
    "sass": "^1.58.0",
    "sass-loader": "^13.2.0",
    "smooth-release": "^8.0.9",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "~4.9.0",
    "webpack": "^5.76.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18"
  },
  "overrides": {
    "lodash": "^4.17.21",
    "cross-spawn": "^7.0.3",
    "got": "^11.8.6",
    "tar": "^6.2.1",
    "webpack-dev-server": "^5.0.4",
    "markdown-to-jsx": "^7.4.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/tests/setup.js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(.*[.](test))[.](tsx?)$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ]
  }
}
